<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias type="mif.mybatis.model.Country" alias="Country"/>
        <typeAlias type="mif.mybatis.model.City" alias="City"/>
        <typeAlias type="mif.mybatis.model.Neighbours" alias="Neighbours"/>
    </typeAliases>
    <environments default="development">
        <environment id="development">
            <transactionManager type="MANAGED"/>
            <dataSource type="JNDI">
                <property name="data_source" value="wm1DataSource"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="mybatis.xml/CountryMapper.xml"/>
        <mapper resource="mybatis.xml/CityMapper.xml"/>
        <mapper resource="mybatis.xml/NeighboursMapper.xml"/>
    </mappers>
</configuration>
